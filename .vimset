let proj_path=$PWD
echo proj_path
let build_command_string="!clear;cd ".proj_path."/build;cmake ..;mv compile_commands.json ../"
let compile_command_string="!clear;cd ".proj_path."/build;make"
let run_command_string="!clear;".proj_path."/build/out"
nnoremap py :vs ~/proj/MagicCube/magic.py<cr>
nnoremap \\ :execute build_command_string<cr>
nnoremap <F3> :execute compile_command_string<cr>
nnoremap <F2> :execute run_command_string<cr>
echo "Compeleted."
